{
    "name": "revise/prime-cms",
    "type": "package",
    "description": "Laravel Based CMS",
    "authors": [{
            "name": "Aras Şanıvar",
            "email": "dev.arassanivar@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Tevfik Mert Çağıran",
            "email": "tmertc@gmail.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "framework",
        "laravel",
        "cms"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^8.0",
        "unisharp/laravel-filemanager": "2.2.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": ">=1.0",
        "google/apiclient": "^2.0",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": ">=2.0"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "CMS\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CMS\\Providers\\CMS"
            ]
        }
    }
}