{
    "name": "revise/prime-cms",
    "type": "project",
    "description": "Laravel Based CMS",
    "authors": [
        {
            "name": "Aras Şanıvar , Tevfik Mert Çağıran",
            "email": "dev.arassanivar@gmail.com"
        }
    ],
    "keywords": [
        "framework",
        "laravel",
        "cms"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "ckfinder/ckfinder-laravel-package": "v3.5.1.4",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "google/apiclient": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "unisharp/laravel-filemanager": "^2.1"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "CMS\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CMS\\Providers\\CMS",
            ]
        }
    }
}
